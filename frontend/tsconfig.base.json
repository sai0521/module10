{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["DOM", "ESNext"],
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react-jsx",
      "allowImportingTsExtensions": true
    }
  }
  